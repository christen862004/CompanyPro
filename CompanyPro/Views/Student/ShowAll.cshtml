<!--Strong Type View-->
@model List<Student>
<!--Display Data Coming From Controll List<student>-->
<table class="table table-bordered table-hover">
    <tr>
        <th>Id</th>
        <th>NAmr</th>
        <th>Image</th>
        <th>Image</th>

    </tr>
    
    @foreach(Student item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.ImageUrl</td>
            <td><img src="~/Images/@item.ImageUrl" style="width:100px" /></td>
        </tr>
    }
</table>
